#include <stdio.h>

// Function declaration
int factorial(int n);

int main() {
    int number = 5;
    printf("Factorial of %d is %d\n", number, factorial(number));
    return 0;
}

// Function definition
int factorial(int n) {
    if(n <= 1) {
        return 1;  // Base case: factorial of 0 or 1 is 1
    } else {
        return n * factorial(n - 1);  // Recursive call
    }
}
