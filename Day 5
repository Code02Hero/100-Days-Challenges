#include <stdio.h>

int main() {
    // For loop example
    printf("For loop:\n");
    for(int i = 1; i <= 10; i++) {
        printf("%d ", i);
    }
    printf("\n");

    // While loop example
    printf("While loop:\n");
    int j = 1;
    while(j <= 10) {
        printf("%d ", j);
        j++;
    }
    printf("\n");

    // Do-while loop example
    printf("Do-while loop:\n");
    int k = 1;
    do {
        printf("%d ", k);
        k++;
    } while(k <= 10);
    printf("\n");

    return 0;
}
